{
  "expo": {
    "name": "PicMap",
    "slug": "PicMap",
    "scheme": "PicMap-routing",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "backgroundColor": "#ffffff",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyAjnI7celPmQ6A6Aa2gkiaKvlHO8cK-2NA"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dingdong.picmap",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCjT7KXZ3bIQ58gxSZQxSnVkP-yg_uNoHw"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": ["expo-router"],
    "experiment": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      }
    },
    "expo": {
      "plugins": [
        [
          "expo-image-picker",
          {
            "photosPermission": "The app accesses your photos to let you share them with your friends.",
            "cameraPermission": "The app accesses your camera to let you share photos with your friends."
          }
        ]
      ]
    }
  }
}
